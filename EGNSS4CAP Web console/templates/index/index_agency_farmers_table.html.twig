{% for user in users %}
  <tr class="clickable" id="{{ user.id }}">
    <td class="js_farmer_tasks" data-label="{{id}}" data-href="{{ user.id }}">{{ user.id }}</td>
    <td class="js_farmer_tasks td_name" data-label="{{name}}" data-href="{{ user.id }}">{{ user.name }}</td>
    <td class="js_farmer_tasks" data-label="{{surname}}" data-href="{{ user.id }}">{{ user.surname }}</td>
    <td class="js_farmer_tasks" data-label="{{ji}}" data-href="{{ user.id }}">{{ user.identification_number }}</td>
    <td class="js_farmer_tasks" data-label="{{tasks_count_text}}" data-href="{{ user.id }}">{{ user.tasks_count }}</td>
    <td class="js_farmer_tasks" data-label="{{photos_count_text}}" data-href="{{ user.id }}">{{ user.photos_count }}</td>
    <td class="js_farmer_tasks" data-label="{{unassigned_photos_count_text}}" data-href="{{ user.id }}">{{ user.unassigned_photos_count }}</td>
    <td class="js_farmer_tasks" data-label="{{tasks_provided_count_text}}" data-href="{{ user.id }}">{{ user.tasks_provided_count }}</td>
    {% if agency %}
      <td data-label="{{action}}"><button type="button" class="js_button_open_user_edit btn btn-outline-secondary" data-userid="{{ user.id }}" data-userlogin="{{ user.login }}" data-username="{{ user.name }}" data-usersurname="{{ user.surname }}" data-useremail="{{ user.email }}" data-uservat="{{ user.vat }}" data-userin="{{ user.identification_number }}" data-formheader="{{ edit_farmer_header }}">{{ edit_user|upper }}</button></td>
    {% endif %}
  </tr>
{% endfor %}
