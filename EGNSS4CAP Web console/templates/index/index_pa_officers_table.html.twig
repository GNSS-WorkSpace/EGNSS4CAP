{% for officer in list_of_officers %}
<tr class="clickable" id="{{officer.id}}">
  <td>{{ officer.login }}</td>
  <td>{{ officer.name }}</td>
  <td>{{ officer.surname }}</td>
  <td>
    <button type="button" class="js_button_open_user_edit btn btn-secondary" data-userid="{{ officer.id }}" data-userlogin="{{ officer.login }}" data-username="{{ officer.name }}" data-usersurname="{{ officer.surname }}" data-useremail="{{ officer.email }}" data-uservat="{{ officer.vat }}" data-userin="{{ officer.identification_number }}" data-formheader="{{ edit_farmer_header }}">{{ edit_user }}</button>
    <button type="button" class="js_button_user_deactivate btn btn-secondary" data-userid="{{ officer.id }}">{{ deactivate_user }}</button>
  </td>
</tr>
{% endfor %}
